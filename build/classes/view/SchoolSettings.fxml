                     <children>
                        <RadioButton fx:id="RdYesCC" mnemonicParsing="false" text="Si" />
                        <RadioButton fx:id="RdNoCC" mnemonicParsing="false" text="No" />
                     </children>
                  </HBox>
               </children>
            </GridPane>
            <TableView fx:id="TableDocumentation" prefHeight="149.0" prefWidth="645.0" />
