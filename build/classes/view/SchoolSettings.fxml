        <URL value="@schoolsettings.css" />
    </stylesheets>
   <children>
      <HBox layoutX="1.0" layoutY="90.0" prefHeight="190.0" prefWidth="1194.0">
         <children>
            <GridPane prefHeight="190.0" prefWidth="326.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
